<ul class="floatleft">
    {{#unless context.data.can_like}}
    <li class="like_button" id="{{context.data.type}}_{{context.data.id_content}}_likes"><a href="{{scripturl}}?action=likes;ltype={{context.data.type}};sa=like;like={{context.data.id_content}};{{context.session_var}}={{context.session_id}}" class="{{context.data.type}}_like"><span class="generic_icons {{#if context.data.already_liked}}unlike{{else}}like{{/if}}"></span> {{#if context.data.already_liked}}{{txt.unlike}}{{else}}{{txt.like}}{{/if}}</a></li>
    {{/unless}}
    {{#if (gt context.data.count 0)}}
    	<li class="like_count smalltext"><a href="?action=likes;sa=view;ltype={{scripturl}}ltype={{context.data.type}};js=1;like={{context.data.id_content}};{{context.session_var}}={{context.session_id}}">{{text_template (get_text context.text) context.data.count}}</a></li>
    {{/if}}
</ul>